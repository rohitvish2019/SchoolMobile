</head>

<body>
    <div class="header-container">
        <%- include('_header'); %>
    </div>
    <div class="body-container">
        <div class="left-side-panel"></div>
        <div class="center-body">
            <div style="margin-left: 10%; margin-bottom: 5%; color: goldenrod;">
                <h1 style="text-align: center; color: white;">
                    Annual Class Upgrade
                </h1>
            </div>
            <select required id="ClassForSearch" style="">
                <option value="" selected disabled>-Select-</option>
                <%for(let i=0;i<classList.length;i++){%>
                    <option><%=classList[i]%></option>
                <%}%>
                
                
            </select>
            <div class="fixTableHead">
                <table class="student-table" id="classList">
                    <thead>
                        <tr>
                            <th></th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
        <div class="right-side-panel"></div>
    </div>
    <div class="footer-container"></div>
